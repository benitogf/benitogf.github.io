<?xml version='1.0' encoding='utf-8'?>
<widget id="com.benitogf.github.io" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BenitoGF</name>
    <server>
        <host>localhost</host>
        <port>8008</port>
    </server>
    <client>
        <host>localhost</host>
        <port>8008</port>
        <tags>list</tags>
        <gapi>gapi</gapi>
        <ganalytics>ganalytics</ganalytics>
        <target>riot</target>
    </client>
    <description>Personal projects information.</description>
    <author email="benitogf@outlook.com" href="http://cordova.io">Benito Gomez</author>
    <hook src="hooks/build.js" type="before_build" />
    <hook src="hooks/build.js" type="before_run" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="windows-target-version" value="10" />
</widget>
